{
  "version": "0.2.0",
  "configurations": [
    {
      "request": "launch",
      "name": "Generate SMS code",
      "type": "node",
      "program": "${workspaceFolder}/functions/lib/sms/generate.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--unstable",
        "--inspect-wait",
        "--allow-all"
      ],
      "attachSimplePort": 9229
    },
    {
      "request": "launch",
      "name": "Auth Function",
      "type": "node",
      "program": "${workspaceFolder}/functions/auth/index.ts",
      "cwd": "${workspaceFolder}/functions",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--unstable",
        "--import-map",
        "${workspaceFolder}/functions/import_map.json",
        "--inspect-wait",
        "--allow-all"
      ],
      "envFile": "${workspaceFolder}/functions/.env",
      "attachSimplePort": 9229
    }
  ],
  "compounds": []
}